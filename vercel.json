{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node",
        
        "memory": 1024,
        "maxDuration": 10
        
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/",
        "memory": 1024,
        "maxDuration": 10
        
      }
    ]
    
  }
  